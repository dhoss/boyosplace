[% USE table (photos, rows=4) %]
<h2>Puppy Photos</h2>

<div id="puppy-list">
[% FOREACH column = table.cols %]
<ul>
[% FOREACH photo = column %]
<li>
  <a href="[% c.uri_for("/photo/$photo.photoid/view") %]">
     <img src="[% c.uri_for("/static/photos/$photo.photoid/$photo.path.filename")%]" width="150" height="100"/>
  </a>
</li>
[% END %]
</ul>
[% END %]
</div>
